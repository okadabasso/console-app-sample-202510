{
  "ConnectionStrings": {
    "default": "Data Source=sample.db"
  },
  "SampleSettings": {
    "Name": "Value1",
    "Age": 10,
    "Description": "environment: debug"
  },
  "NLog": {
    "internalLogLevel": "Info",
    "internalLogFile": "c:\\temp\\internal-nlog.txt",
    "extensions": [
      {
        "assembly": "NLog.Extensions.Logging"
      }
    ],
    "targets": {
      "file": {
        "type": "File",
        "fileName": "logs\\log-${shortdate}.log",
        "layout": "${longdate}|[${uppercase:${level:padding=-5}}] [${threadid}] ${callsite}#${callsite-linenumber} ${message} ${exception}"
      },
      "console": {
        "type": "Console",
        "layout": "[${uppercase:${level:padding=-5}}] [${threadid}] ${callsite}#${callsite-linenumber} ${message} ${exception}"
      },
      "null": {
        "type": "Null"
      }
    },
    "rules": [
      {
        "logger": "Microsoft.EntityFrameworkCore.*",
        "minLevel": "Trace",
        "writeTo": "null",
        "final": true
      },
      {
        "logger": "*",
        "minLevel": "Trace",
        "writeTo": "file,console"
      }
    ]
  }
}